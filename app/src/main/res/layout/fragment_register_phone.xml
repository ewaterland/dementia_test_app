<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white" >

    <!-- fragment -->
    <FrameLayout
        android:id="@+id/view_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

    </FrameLayout>

    <!-- register2 -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_register"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <!-- 상단바 START -->
        <GridLayout
            android:id="@+id/view_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <!-- 로고 START -->
            <ImageView
                android:id="@+id/image_logo"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_gravity="right"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/icon" />
            <!-- 로고 END -->

        </GridLayout>
        <!-- 상단바 END -->

        <TextView
            android:id="@+id/textView_before"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:gravity="start"
            android:text="이전"
            android:textColor="#000000"
            android:textSize="25sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view_top">

        </TextView>

        <!-- 메인 START -->
        <TextView
            android:id="@+id/textView_subject"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="30dp"
            android:text="연락처를 입력해 주세요"
            android:textColor="#000000"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_before" />

        <!-- 본인 -->
        <TextView
            android:id="@+id/textView_my"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="50dp"
            android:text="본인의 연락처"
            android:textColor="#000000"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_subject" />

        <!-- 본인 핸드폰 번호 입력칸 -->
        <LinearLayout
            android:id="@+id/MyPhone"
            android:layout_width="match_parent"
            android:layout_height="100sp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_my" >

            <EditText
                android:id="@+id/editText_MyFirst"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:gravity="center"
                android:hint="010"
                android:inputType="number"
                android:textSize="30sp"
                android:maxLength="3">

            </EditText>

            <TextView
                android:id="@+id/text_My1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:gravity="center"
                android:text=" - "
                android:textSize="30sp">

            </TextView>

            <EditText
                android:id="@+id/editText_MyMiddle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:gravity="center"
                android:hint="0000"
                android:inputType="number"
                android:textSize="30sp"
                android:maxLength="4">

            </EditText>

            <TextView
                android:id="@+id/text_My2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text=" - "
                android:textSize="30sp">

            </TextView>

            <EditText
                android:id="@+id/editText_MyLast"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:gravity="center"
                android:hint="0000"
                android:inputType="number"
                android:textSize="30sp"
                android:maxLength="4">

            </EditText>
        </LinearLayout>

        <!-- 보호자 -->
        <TextView
            android:id="@+id/textView_guardian"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="15dp"
            android:text="보호자의 연락처"
            android:textColor="#000000"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/MyPhone" />

        <!-- 보호자 핸드폰 번호 입력칸 -->
        <LinearLayout
            android:id="@+id/GuardianPhone"
            android:layout_width="match_parent"
            android:layout_height="100sp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_guardian" >

            <EditText
                android:id="@+id/editText_GuardianFirst"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:gravity="center"
                android:hint="010"
                android:inputType="number"
                android:textSize="30sp"
                android:maxLength="3" >

            </EditText>

            <TextView
                android:id="@+id/text_Guardian1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:gravity="center"
                android:text=" - "
                android:textSize="30sp">

            </TextView>

            <EditText
                android:id="@+id/editText_GuardianMiddle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:gravity="center"
                android:hint="0000"
                android:inputType="number"
                android:textSize="30sp"
                android:maxLength="4" >

            </EditText>

            <TextView
                android:id="@+id/text_Guardian2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:gravity="center"
                android:text=" - "
                android:textSize="30sp">

            </TextView>

            <EditText
                android:id="@+id/editText_GuardianLast"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@null"
                android:gravity="center"
                android:hint="0000"
                android:inputType="number"
                android:textSize="30sp"
                android:maxLength="4" >

            </EditText>
        </LinearLayout>

        <TextView
            android:id="@+id/text_next"
            android:layout_width="347dp"
            android:layout_height="56dp"
            android:layout_marginBottom="40dp"
            android:fontFamily="@font/inter_semibold"
            android:gravity="right"
            android:text="다음"
            android:textColor="#D7D7D7"
            android:textSize="30dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <!-- 메인 END -->

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>